{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console":"off",
        "linebreak-style":"off",
        "no-param-reassign":"off",
        "no-unused-vars":"off",
        "no-undef":"off",
        "consistent-return":"off",
        "no-underscore-dangle":"off",
        "func-names":"off",
        "no-return-await":"off"
    }
}
